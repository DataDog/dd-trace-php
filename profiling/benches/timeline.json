{
    "name": "php-profiler-timeline-memory",
    "run": "php -d extension=../target/release/libdatadog_php_profiling.so benches/timeline.php",
    "timeout": 20,
    "iterations": 5,
    "env": {
        "DD_PROFILING_LOG_LEVEL": "off"
    },
    "variants": {
        "control": {
            "env": {
                "DD_PROFILING_ENABLED": "false"
            }
        },
        "with-profiler": {
            "env": {
                "DD_PROFILING_ENABLED": "true",
                "DD_PROFILING_EXPERIMENTAL_TIMELINE_ENABLED": "false"
            }
        },
        "with-profiler-and-timeline": {
            "env": {
                "DD_PROFILING_ENABLED": "true",
                "DD_PROFILING_EXPERIMENTAL_TIMELINE_ENABLED": "true"
            }
        }
    }
}
