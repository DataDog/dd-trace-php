<entities xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:mftf:DataGenerator/etc/dataProfileSchema.xsd">
    <entity name="Wishlist" type="wishlist">
        <data key="id">null</data>
        <var key="product" entityType="product" entityKey="id"/>
        <var key="customer_email" entityType="customer" entityKey="email"/>
        <var key="customer_password" entityType="customer" entityKey="password"/>
        <data key="shareInfo_emails">JohnDoe123456789@example.com,JohnDoe987654321@example.com,JohnDoe123456abc@example.com</data>
        <data key="shareInfo_message">Sharing message.</data>
        <data key="default_email_text_length_limit">255</data>
        <data key="min_email_text_length_limit">1</data>
        <data key="max_email_text_length_limit">10000</data>
    </entity>
    <entity name="notValidEmails" type="wishlist">
        <data key="id">null</data>
        <var key="product" entityType="product" entityKey="id"/>
        <var key="customer_email" entityType="customer" entityKey="email"/>
        <var key="customer_password" entityType="customer" entityKey="password"/>
        <data key="shareInfo_emails">JohnDoe123456789@,JohnDoe987654321example.com,JohnDoe123456abc@@example.com</data>
        <data key="shareInfo_message">Sharing message.</data>
        <data key="default_email_text_length_limit">255</data>
        <data key="min_email_text_length_limit">1</data>
        <data key="max_email_text_length_limit">10000</data>
    </entity>
    <entity name="WishlistEnable">
        <data key="path">wishlist/general/active</data>
        <data key="value">1</data>
    </entity>
    <entity name="WishlistDisable">
        <data key="path">wishlist/general/active</data>
        <data key="value">0</data>
    </entity>
    <entity name="WishlistShowInSidebar">
        <data key="path">wishlist/general/show_in_sidebar</data>
        <data key="value">1</data>
    </entity>
    <entity name="WishlistHideFromSidebar">
        <data key="path">wishlist/general/show_in_sidebar</data>
        <data key="value">0</data>
    </entity>
    <entity name="DisableRssConfig">
        <data key="path">rss/config/active</data>
        <data key="value">0</data>
    </entity>
    <entity name="DisableWishlistRssConfig">
        <data key="path">rss/wishlist/active</data>
        <data key="value">0</data>
    </entity>
    <entity name="EnableRssConfig">
        <data key="path">rss/config/active</data>
        <data key="value">1</data>
    </entity>
    <entity name="EnableWishlistRssConfig">
        <data key="path">rss/wishlist/active</data>
        <data key="value">1</data>
    </entity>
</entities>
