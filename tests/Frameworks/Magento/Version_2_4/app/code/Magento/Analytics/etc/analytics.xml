<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Analytics:etc/analytics.xsd">
    <file name="modules">
        <providers>
            <reportProvider name="modules" class="Magento\Analytics\ReportXml\ReportProvider">
                <parameters>
                    <name>modules</name>
                </parameters>
            </reportProvider>
        </providers>
    </file>
    <file name="store_config">
        <providers>
            <customProvider name="store_config" class="Magento\Analytics\Model\StoreConfigurationProvider"/>
        </providers>
    </file>
    <file name="stores">
        <providers>
            <reportProvider name="stores" class="Magento\Analytics\ReportXml\ReportProvider">
                <parameters>
                    <name>stores</name>
                </parameters>
            </reportProvider>
        </providers>
    </file>
    <file name="websites">
        <providers>
            <reportProvider name="websites" class="Magento\Analytics\ReportXml\ReportProvider">
                <parameters>
                    <name>websites</name>
                </parameters>
            </reportProvider>
        </providers>
    </file>
    <file name="groups">
        <providers>
            <reportProvider name="groups" class="Magento\Analytics\ReportXml\ReportProvider">
                <parameters>
                    <name>groups</name>
                </parameters>
            </reportProvider>
        </providers>
    </file>
</config>
