<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Sales:etc/pdf_file.xsd">
    <renderers>
        <page type="type_one">
            <renderer product_type="product_type_one">Renderer\Type\One\Product\One</renderer>
            <renderer product_type="product_type_two">Renderer\Type\One\Product\Two</renderer>
        </page>
        <page type="type_two">
            <renderer product_type="product_type_three">Renderer\Type\Two\Product\Two</renderer>
        </page>
    </renderers>
    <totals>
        <total name="total1">
            <title>Title1 Modified</title>
            <source_field>source1</source_field>
            <title_source_field>title_source1</title_source_field>
            <font_size>1</font_size>
            <display_zero>false</display_zero>
            <sort_order>1</sort_order>
            <model>Model1</model>
            <amount_prefix>prefix1</amount_prefix>
        </total>
        <total name="total2">
            <title>Title2</title>
            <source_field>source2</source_field>
            <title_source_field>title_source2</title_source_field>
            <font_size>2</font_size>
            <display_zero>true</display_zero>
            <sort_order>2</sort_order>
            <model>Model2</model>
            <amount_prefix>prefix2</amount_prefix>
        </total>
        <total name="total3">
            <title>Title3</title>
            <source_field>source3</source_field>
            <title_source_field>title_source3</title_source_field>
            <font_size>3</font_size>
            <display_zero>false</display_zero>
            <sort_order>3</sort_order>
            <model>Model3</model>
            <amount_prefix>prefix3</amount_prefix>
        </total>
    </totals>
</config>
