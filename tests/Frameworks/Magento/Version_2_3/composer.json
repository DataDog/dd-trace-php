    "scripts": {
        "pre-update-cmd": [
            "unzip -qn magento.zip"
        ],
        "post-update-cmd": [
            "sudo find var generated vendor pub/static pub/media app/etc -type f -exec chmod g+w {} +\n",
            "sudo find var generated vendor pub/static pub/media app/etc -type d -exec chmod g+ws {} +\n",
            "sudo chmod u+x bin/magento",
            "sudo chmod u+x install-magento",
            "sudo ./install-magento",
            "sudo chmod -R 777 var generated pub/static pub/media"
        ]
    }
