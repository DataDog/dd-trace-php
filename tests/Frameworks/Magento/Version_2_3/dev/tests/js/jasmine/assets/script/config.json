{
    "base": {
        "containerId": "test-suite-container",
        "dataAttr": "data-mage-init"
    },
    "virtual": {
        "scriptData": {
            "*": {
                "tests/assets/script/component": {
                    "config": {
                        "foo": "bar"
                    }
                }
            }
        },
        "scriptId": "virtual-selector"
    },
    "bySelector": {
        "nodeData": {
            "tests/assets/script/component": {
                "data": {
                    "foo": {
                        "baz": "foo"
                    }
                }
            }
        },
        "scriptData": {
            "#node-by-selector": {
                "tests/assets/script/component": {
                    "data": {
                        "foo": {
                            "bar": "baz"
                        }
                    }
                }
            }
        },
        "merged": {
            "tests/assets/script/component": {
                "data": {
                    "foo": {
                        "baz": "foo",
                        "bar": "baz"
                    }
                }
            }
        },
        "nodeId": "node-by-selector"
    }
}