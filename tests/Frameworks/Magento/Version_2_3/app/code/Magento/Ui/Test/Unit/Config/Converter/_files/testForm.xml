<form xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Ui:etc/ui_configuration.xsd">
    <settings>
        <buttons/>
    </settings>
    <htmlContent name="customer_edit_tab_view_content">
        <block class="Magento\Customer\Block\Adminhtml\Edit\Tab\View"
               name="customer_edit_tab_view" template="Magento_Customer::tab/view.phtml">
            <block class="Magento\Customer\Block\Adminhtml\Edit\Tab\View\PersonalInfo"
                   name="personal_info" template="Magento_Customer::tab/view/personal_info.phtml"/>
        </block>
    </htmlContent>
    <fieldset name="fieldset">
        <settings>
            <additionalClasses>
                <class name="classNameOne">true</class>
                <class name="classNameTwo">false</class>
            </additionalClasses>
            <links>
                <link name="propertyOne">valueOne</link>
                <link name="propertyTwo">valueTwo</link>
            </links>
            <imports>
                <link name="propertyOne">valueOne</link>
                <link name="propertyTwo">valueTwo</link>
            </imports>
            <exports>
                <link name="propertyOne">valueOne</link>
                <link name="propertyTwo">valueTwo</link>
            </exports>
            <listens>
                <link name="propertyOne">valueOne</link>
                <link name="propertyTwo">valueTwo</link>
            </listens>
        </settings>
    </fieldset>
</form>
