        <analytics>
            <url>
                <signup>https://advancedreporting.rjmetrics.com/signup</signup>
                <update>https://advancedreporting.rjmetrics.com/update</update>
                <bi_essentials>https://dashboard.rjmetrics.com/v2/magento/signup</bi_essentials>
