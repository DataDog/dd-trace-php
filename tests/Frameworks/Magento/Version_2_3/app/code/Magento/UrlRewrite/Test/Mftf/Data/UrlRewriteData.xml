<entities xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
          xsi:noNamespaceSchemaLocation="urn:magento:mftf:DataGenerator/etc/dataProfileSchema.xsd">
    <entity name="defaultUrlRewrite" type="urlRewrite">
        <data key="request_path" unique="prefix">test-test-test.html</data>
        <data key="target_path">http://www.example.com/</data>
        <data key="redirect_type">302</data>
        <data key="redirect_type_label">Temporary (302)</data>
        <data key="store_id">1</data>
        <data key="store">Default Store View</data>
