<entities xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
          xsi:noNamespaceSchemaLocation="urn:magento:mftf:DataGenerator/etc/dataProfileSchema.xsd">
    <entity name="AdminAccountSharingYes" type="admin_account_sharing_value">
        <data key="value">Yes</data>
    </entity>
    <entity name="AdminAccountSharingNo" type="admin_account_sharing_value">
        <data key="value">No</data>
    </entity>
    <entity name="EnableAdminAccountSharing" type="admin_account_sharing_config">
        <requiredEntity type="admin_account_sharing_value">AdminAccountSharingYes</requiredEntity>
    </entity>
    <entity name="DisableAdminAccountSharing" type="admin_account_sharing_config">
        <requiredEntity type="admin_account_sharing_value">AdminAccountSharingNo</requiredEntity>
    </entity>
</entities>
