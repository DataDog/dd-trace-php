<listing xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Ui:etc/ui_configuration.xsd">
    <listingToolbar name="listing_top"/>
    <columns name="product_attributes_columns">
        <column name="is_filterable" component="Magento_Ui/js/grid/columns/select">
            <settings>
                <options class="Magento\LayeredNavigation\Model\Attribute\Source\FilterableOptions"/>
                <filter>select</filter>
                <dataType>select</dataType>
                <label translate="true">Use in Layered Navigation</label>
            </settings>
        </column>
    </columns>
</listing>
