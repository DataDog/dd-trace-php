<form xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Ui:etc/ui_configuration.xsd">
    <argument name="data" xsi:type="array">
        <item name="childrenFromMeta" xsi:type="boolean">true</item>
    </argument>
    <fieldset name="data">
        <field name="experiment_script" sortOrder="100" formElement="textarea">
            <settings>
                <dataType>text</dataType>
                <label translate="true">Experiment Code</label>
                <dataScope>data.google_experiment.experiment_script</dataScope>
                <componentType>field</componentType>
            </settings>
        </field>
        <field name="code_id" formElement="input">
            <settings>
                <dataType>text</dataType>
                <label translate="false"/>
                <visible>false</visible>
                <dataScope>data.google_experiment.code_id</dataScope>
                <componentType>field</componentType>
            </settings>
        </field>
    </fieldset>
</form>
